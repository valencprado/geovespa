<head>
  <meta charset="UTF-8">
  <meta content="width=device-width, initial-scale=1, maximum-scale=1, shrink-to-fit=no" name="viewport">
  <title>Mapa Vespa</title>
  <!-- General CSS Files -->
  <link rel="stylesheet" href="/assets/css/app.min.css">
  <!-- Template CSS -->
  <link rel="stylesheet" href="/assets/css/style.css">
  <link rel="stylesheet" href="/assets/css/components.css">
  <!-- Custom style CSS -->
  <link rel="stylesheet" href="/assets/bundles/izitoast/css/iziToast.min.css">

  <link rel="stylesheet" href="/assets/css/custom.css">
  <link rel="icon" type="image/x-icon" href="/images/icon.png" />

  <link rel="stylesheet" href="/assets/bundles/datatables/datatables.min.css">
  <link rel="stylesheet" href="/assets/bundles/datatables/DataTables-1.10.16/css/dataTables.bootstrap4.min.css">

  <script src="https://unpkg.com/leaflet@1.9.4/dist/leaflet.js"
  integrity="sha256-20nQCchB9co0qIjJZRGuk2/Z9VM+kNiyxNV1lvTlZBo="
  crossorigin=""></script>
</head>

<body>
  <div id="app">
    <nav class="navbar navbar-expand-lg main-navbar sticky navbar_vespa">
      <div class="form-inline mr-auto">
        <ul class="navbar-nav mr-3">
          <li>
            <a href="/">
              <img style="width: 130px;" src="/images/logo.png" alt="logo" class="w-full">
          </a>
       
          </li>


        </ul>
      </div>
      <ul class="navbar-nav navbar-right">
        <li class="dropdown"><a href="#" data-toggle="dropdown"
            class="nav-link dropdown-toggle nav-link-lg nav-link-user"> <img alt="image" src="/assets/img/user.png"
              class="user-img-radious-style"> <span class="d-sm-none d-lg-inline-block"></span></a>
          <div class="dropdown-menu dropdown-menu-right pullDown">
            <div class="dropdown-title">{{user.username}}</div>
            <a href="/dashboard/perfil" class="dropdown-item has-icon"> <i class="far
                  fa-user"></i> Perfil
            </a> 
            <a href="/dashboard" class="dropdown-item has-icon"> <i class="far
                  fa-map"></i> GeoVespa
            </a> 
            <div class="dropdown-divider"></div>
            <a href="/auth/logout" class="dropdown-item has-icon text-danger"> <i class="fas fa-sign-out-alt"></i>
              Sair
            </a>
          </div>
        </li>
      </ul>
    </nav>
    <section style="margin-top: 80px;">

      <div class="container-fluid">

        <div class="row mt-sm-4">
          <div class="col-12 col-md-12 col-lg-4">
            <div class="card author-box">
              <div class="card-body">
                <div class="author-box-center">
                
                  <div class="clearfix"></div>
                  <div class="author-box-name">
                    <a href="#">{{user.username}}</a>
                  </div>
                  <div class="author-box-job">Autenticado</div>
                </div>

              </div>
            </div>
            <div class="card">
              <div class="card-header">
                <h4>Informações</h4>
              </div>
              <div class="card-body">
                <div class="py-4">
                  <p class="clearfix">
                    <span class="float-left">
                      Email
                    </span>
                    <span class="float-right text-muted">
                      {{user.email}}
                    </span>
                  </p>
                  <p class="clearfix">
                    <span class="float-left">
                      Username
                    </span>
                    <span class="float-right text-muted">
                      {{user.username}}
                    </span>
                  </p>
 
                </div>
              </div>
            </div>

          </div>
          <div class="col-12 col-md-12 col-lg-8">
            <div class="card">
              <div class="padding-20">
                <ul class="nav nav-tabs" id="myTab2" role="tablist">
                  <li class="nav-item">
                    <a class="nav-link active" id="home-tab2" data-toggle="tab" href="#about" role="tab"
                      aria-selected="true">Configuração</a>
                  </li>
                </ul>
                <div class="tab-content tab-bordered" id="myTab3Content">
                  <div class="tab-pane fade show active" id="about" role="tabpanel" aria-labelledby="home-tab2">

                    <form method="post" class="needs-validation">
                      <div class="card-header">
                        <h4>Editar Perfil</h4>
                      </div>
                      <div class="card-body">
                        <div class="row">
                          <div class="form-group col-md-6 col-12">
                              <label>Username</label>
                              <input v-model="user.username" type="text" class="form-control" value="">
                              <div class="invalid-feedback">
                                  nome do usuário
                              </div>
                          </div>
                          <div class="form-group col-md-6 col-12">
                              <label>Email</label>
                              <input v-model="user.email" type="email" class="form-control" value="">
                              <div class="invalid-feedback">
                                  Please fill in the last name
                              </div>
                          </div>

                          
                          <div class="col">
                            <button style="float: right;" @click="edit"  class="btn btn-primary">Guardar</button>
                          </div>
                      </div>
                       
                        <hr>
                        <div class="row">
                          <div class="form-group col-4">
                              <label>Senha Atual</label>
                              <input v-model="currentPassword" type="password" class="form-control" value="">
                          </div>
                          <div class="form-group col-4">
                              <label>Nova Senha</label>
                              <input v-model="password" type="password" class="form-control" value="">
                          </div>
                          <div class="form-group col-4">
                              <label>Confirmação da Senha</label>
                              <input v-model="password1" type="password" class="form-control" value="">
                          </div>
                      </div>

                        <button style="float: right;" v-if="password&&password==password1" @click="senha" class="btn btn-primary">Mudar Senha</button>

                      </div>

                    </form>
                  </div>
 
                </div>
              </div>
            </div>
          </div>
        </div>

      </div>
    </section>
  </div>


  <!-- General JS Scripts -->
  <script src="/assets/js/app.min.js"></script>
  <!-- JS Libraies -->
  <!-- Page Specific JS File -->
  <!-- Template JS File -->
  <script src="/assets/js/scripts.js"></script>
  <script src="/assets/bundles/sweetalert/sweetalert.min.js"></script>
  <script src="/assets/bundles/izitoast/js/iziToast.min.js"></script>
  <!-- Custom JS File -->
  <script src="https://unpkg.com/vue@3/dist/vue.global.js "></script>
  <script src="/assets/bundles/sweetalert/sweetalert.min.js"></script>
  <script src="/assets/bundles/izitoast/js/iziToast.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/axios/1.3.4/axios.min.js"></script>

  <script>
      const jwt = '<%= jwt %>'

      let user = '<%- JSON.stringify(user) %>'

      user = JSON.parse(user)
      console.log(user)
  </script>

  <script type="module" src="/assets/js/perfil.js"></script>
</body>


